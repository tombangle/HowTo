    "@react-navigation/native": "^7.1.6",
    "@supabase/supabase-js": "^2.49.4",
    "expo": "~53.0.9",
    "expo-asset": "~11.1.7",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.7",
    "expo-image-picker": "~16.0.2",
    "expo-linking": "~7.1.5",
