{
  "name": "decision-tree-app",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "fix:public": "node scripts/fix-public-path.js",
    "deploy:pages": "gh-pages -d dist",
    "deploy:web": "bash scripts/deploy-web.sh",
    "deploy:docs": "rm -rf docs && mkdir docs && cp -R dist/* docs && git add docs && git commit -m \"Deploy to docs\" && git push",
    "export:web": "npx expo export --platform web --output-dir dist",
    "postexport:web": "node scripts/patch-index.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
